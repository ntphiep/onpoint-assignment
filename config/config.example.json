{
  "data_sources": {
    "api": {
      "jsonplaceholder": {
        "base_url": "https://jsonplaceholder.typicode.com",
        "endpoints": {
          "posts": "/posts",
          "users": "/users",
          "comments": "/comments"
        }
      }
    },
    "web": {
      "reddit": {
        "url": "https://www.reddit.com/r/python.json",
        "selectors": {
          "posts": ".thing",
          "title": ".title a",
          "score": ".score",
          "author": ".author",
          "comments": ".comments"
        }
      }
    }
  },
  "aws": {
    "region": "us-east-1",
    "s3_bucket": "onpoint-assessment-bucket",
    "access_key": "DEMO_ACCESS_KEY",
    "secret_key": "DEMO_SECRET_KEY"
  },
  "database": {
    "postgresql": {
      "host": "localhost",
      "port": 5432,
      "database": "onpoint_assessment",
      "username": "postgres",
      "password": "password"
    }
  },
  "processing": {
    "batch_size": 1000,
    "max_workers": 4,
    "timeout": 30
  },
  "file_formats": {
    "csv": {
      "encoding": "utf-8",
      "separator": ","
    },
    "parquet": {
      "compression": "snappy",
      "engine": "pyarrow"
    },
    "excel": {
      "engine": "openpyxl"
    }
  }
}